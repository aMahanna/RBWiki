{
  "name": "rubes-wiki",
  "version": "1.0.1",
  "description": "Photo-enabled wikipedia searching using Google Vision & Wikijs for LP records",
  "main": "server.js",
  "scripts": {
    "start": "rm -r -f public/images && mkdir public/images && node dist/server.js",
    "dev": "concurrently \"yarn run watch:build\" \"yarn run watch:server\"",
    "watch:build": "tsc --watch",
    "watch:server": "yarn run clean:images && NODE_ENV=test nodemon ./dist/server.js --watch ./dist",
    "clean:dist": "rm -r -f dist",
    "clean:images": "rm -r -f public/images && mkdir public/images",
    "build": "tsc",
    "postinstall": "tsc"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/hbs": "^4.0.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/request-promise": "^4.1.47",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "hbs": "^4.0.1",
    "http": "^0.0.1-security",
    "multer": "^1.3.0",
    "node": "^15.8.0",
    "node-fetch": "^2.6.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.5",
    "uuid": "^3.2.1",
    "wikijs": "^6.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "^15.5.0",
    "yarn": "^1.22.10"
  },
  "repository": {
    "url": "https://github.com/aMahanna/rubeswiki"
  },
  "license": "MIT"
}
